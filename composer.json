{
    "name": "codica-studio/phonevalidator",
    "description": "A random Codica Studio package.",
    "keywords": [
        ""
    ],
    "homepage": "https://github.com/codica-studio/phonevalidator",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "mbrown@codicastudio.com",
            "homepage": "https://codicastudio.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "illuminate/support": "^8.0",
        "illuminate/validation": "^8.0",
        "giggsey/libphonenumber-for-php": "^8.0",
        "league/iso3166": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Propaganistas\\LaravelPhone\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Propaganistas\\LaravelPhone\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Propaganistas\\LaravelPhone\\PhoneServiceProvider"
            ]
        }
    }
}
